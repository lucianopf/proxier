{
  "version": 2,
  "name": "proxier",
  "alias": ["proxier.now.sh"],
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/.*",
      "methods": ["OPTIONS"],
      "dest": "200.js"
    },
    {
      "src": "/api",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "*"
      },
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "*"
      },
      "dest": "/api?url=$1"
    }
  ]
}